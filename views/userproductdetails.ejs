<script>
   

    function addtocart(price) {
        const id = document.getElementById('cart').value

        $.ajax({
            url: "/usercart",
            method: "post",
            data: {
                id,
                price
            },
            success: (response) => {
                if (response.status === "viewcart") {
                   
                    Swal.fire({
                        title: "Good job!",
                        text: "Product added to cart!",
                        icon: "success",
                        customClass: {
                            confirmButton: 'black-ok-button'
                        },
                        iconColor: '#3F2520'
                    }) .then((result)=>{
                        window.location.reload()
                     })

                     
                  
                } else if (response.status === true) {
                    Swal.fire({
                        title: "Good job!",
                        text: "Product added to cart!",
                        icon: "success",
                        customClass: {
                            confirmButton: 'black-ok-button'
                        },
                        iconColor: '#3F2520'
                    }) .then((result)=>{
                        window.location.reload()
                     })


                  
                }

            }
        })
    }

    function addToWishlist(pid){
       $.ajax({
        url:'/wishlist',
        method:'post',
        data:{
            pid
        },
        success:(response)=>{
           if(response.status === 'viewwishlist'){
            const Toast = Swal.mixin({
                toast: true,
                position: "bottom",
                showConfirmButton: false,
                timer: 3000,
                timerProgressBar: true,
                didOpen: (toast) => {
                    toast.onmouseenter = Swal.stopTimer;
                    toast.onmouseleave = Swal.resumeTimer;
                }
                });
                Toast.fire({
                icon: "success",
                title: "Product added to wishlist",
                color: 'black',
                iconColor:'#3F2520'
                })
               
                
               
           }else if(response.status === true){
            const Toast = Swal.mixin({
                toast: true,
                position: "bottom",
                showConfirmButton: false,
                timer: 3000,
                timerProgressBar: true,
                didOpen: (toast) => {
                    toast.onmouseenter = Swal.stopTimer;
                    toast.onmouseleave = Swal.resumeTimer;
                }
                });
                Toast.fire({
                icon: "success",
                title: "Product added to wishlist",
                color: 'black',
                iconColor:'#3F2520'
                });
           } else if(response.status === 'viewwishlistcheck'){
            const Toast = Swal.mixin({
                toast: true,
                position: "bottom",
                showConfirmButton: false,
                timer: 3000,
                timerProgressBar: true,
                didOpen: (toast) => {
                    toast.onmouseenter = Swal.stopTimer;
                    toast.onmouseleave = Swal.resumeTimer;
                }
                });
                Toast.fire({
                icon: "warning",
                title: "Product is already in wishlist",
                color: 'black',
                iconColor:'#3F2520'
                });
           }
        }

       })
    }

</script>

