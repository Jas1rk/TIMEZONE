  <script>
    function clrname() {
      document.getElementById("error1").textContent = "";
    }
    function clremail() {
      document.getElementById("error2").textContent = "";
    }
    function clrmobile() {
      document.getElementById("error3").textContent = "";
    }
    function clrpassword1() {
      document.getElementById("error4").textContent = "";
    }
    function clrpassword2() {
      document.getElementById("error5").textContent = "";
    }
    const userId = document.getElementById("username");
    const emailId = document.getElementById("email");
    const mobileId = document.getElementById("mobile");
    const passwordID = document.getElementById("pass1");
    const confirmeId = document.getElementById("pass2");
    const error1 = document.getElementById("error1");
    const error2 = document.getElementById("error2");
    const error3 = document.getElementById("error3");
    const error4 = document.getElementById("error4");
    const error5 = document.getElementById("error5");
    const regForm = document.getElementById("form");

    function namevalidate() {
      const nameVali = userId.value.trim();
      const pattern = /^[A-Za-z\s'-]{1,}$/;

      if (nameVali === "") {
        error1.style.display = "block";
        error1.innerHTML = "Enter a valid name";
      } else if (!pattern.test(nameVali)) {
        error1.style.display = "block";
        error1.innerHTML = "Invalid format First letter should be capital";
      } else {
        error1.style.display = "none";
        error1.innerHTML = "";
      }
    }

    function emailvalidate() {
      const emailVali = emailId.value;
      const Empattern = /^([a-zA-Z0-9._-]+)@([a-zA-Z.-]+).([a-zA-z]{2,4})$/;
      if (!Empattern.test(emailVali)) {
        error2.style.display = "block";
        error2.innerHTML = "Enter a valid Email";
      } else {
        error2.style.display = "none";
        error2.innerHTML = "";
      }
    }

    function mobilevalidate() {
      const mobilevali = mobileId.value.trim();
      if (mobilevali === "") {
        error3.style.display = "block";
        error3.innerHTML = "Enter a valid Number";
      } else if (mobilevali.length !== 10 || /^(.)\1{9}$/.test(mobilevali)) {
        error3.style.display = "block";
        error3.innerHTML = "Number should be 10 digit";
      } else {
        error3.style.disply = "none";
        error3.innerHTML = "";
      }
    }

    function passwordvaidate() {
      const passvali = passwordID.value;
      const passpatern = /[a-zA-Z]/;
      const digit = /\d/;

      if (passvali.length < 6) {
        error4.style.display = "";
        error4.innerHTML = "Must have 6 charecters";
      } else if (!passpatern.test(passvali) || !digit.test(passvali)) {
        error4.style.display = "block";
        error4.innerHTML = "Should contain Numbers and Alphabets";
      }
    }

    function confimePassvalidate() {
      const passvali = passwordID.value;
      const passvalid = confirmeId.value;
      const passpatern = /[a-zA-Z]/;
      const digit = /\d/;

      if (passvalid !== passvali) {
        error5.style.display = "block";
        error5.innerHTML = "Passwords do not match";
      } else if (!passpatern.test(passvalid) || !digit.test(passvalid)) {
        error5.style.display = "block";
        error5.innerHTML = "Should contain Numbers and Alphabets";
      }
    }

    userId.addEventListener("blur", namevalidate);
    emailId.addEventListener("blur", emailvalidate);
    mobileId.addEventListener("blur", mobilevalidate);
    passwordID.addEventListener("blur", passwordvaidate);
    confirmeId.addEventListener("blur", confimePassvalidate);

    regForm.addEventListener("submit", function (e) {
      namevalidate();
      emailvalidate();
      mobilevalidate();
      passwordvaidate();
      confimePassvalidate();

      if (
        error1.innerHTML ||
        error2.innerHTML ||
        error3.innerHTML ||
        error4.innerHTML ||
        error5.innerHTML
      ) {
        e.preventDefault();
      }
    });
  </script>

