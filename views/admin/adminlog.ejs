<body>
    <main>
        <header class="main-header style-2 navbar">
            <div class="col-brand">
                
                <div class="logo logo-width-1" style="width: 110px; height: 100px; overflow: hidden;">
                    <a href="/"><img src="/admin-assets/imgs/theme/timezone Logo.png"  style="min-width: 100% ;"  alt="logo"></a>
                </div>
            </div> 
           
        </header> 
        <section class="content-main mt-80 mb-80">
            <div class="card mx-auto card-login">
                <div class="card-body">
                   
                    <h4 class="card-title mb-4">Log in</h4>
                    <% if (locals.message) { %>
                        <p id="msg" class="text-danger"><%= message %></p>
  
                      <% } %>
                      
                    <form method="post" action="/admin/">
                        <div class="mb-3">
                            <input class="form-control" placeholder="Email" type="text" name="email" value="jskjasir90@gmail.com">
                        </div>
                        <div class="mb-3">
                            <input class="form-control" placeholder="Password" type="password" name="password" value="hello@123">
                        </div> 
                      
                        <div class="mb-4">
                            <button type="submit" class="btn btn-primary w-100"  style="background-color: #3F2520;"> Login </button>
                        </div> 
                    </form>
                  
                   
                    
                </div>
            </div>
        </section>
        
    </main>
    <script src="/admin-assets/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="/admin-assets/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="/admin-assets/js/vendors/jquery.fullscreen.min.js"></script>
    <!-- Main Script -->
    <script src="/admin-assets/js/main.js" type="text/javascript"></script>
</body>
<script>

    setTimeout(() => {
        document.getElementById('msg').style.display="none"
    },2000);
</script>


