<script>
  function blockCoupon(id){
    $.ajax({
        url:'/admin/blockcoupon',
        method:'post',
        data:{
            id
        },
        success:(response)=>{
            if(response.status === 'blocked'){
                window.location.href = '/admin/coupons'
            }
        }
    })
  }

  function UnblockCoupon(id){
    $.ajax({
        url:'/admin/unblockcoupon',
        method:'post',
        data:{
            id
        },
        success:(response)=>{
            if(response.status === 'unblock'){
                window.location.href ='/admin/coupons'
            }
        }

    })
  }
  function DeleteCoupon(id){
    $.ajax({
        url:'/admin/deletecoupon',
        method:'delete',
        data:{
            id
        },
        success:(response)=>{
            if(response.status ==='deleted'){
                window.location.reload()
            }
        }
    })
  }
</script>

